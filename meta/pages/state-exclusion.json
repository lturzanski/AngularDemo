{
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "State Exclusions",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Add",
                  "allowAdd": true,
                  "allowDelete": true,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "filterProperty": "State.StateName",
                      "property": "StateId",
                      "sortProperty": "State.StateName",
                      "template": "${data.State?.StateName}",
                      "title": "State",
                      "type": "string"
                    },
                    {
                      "filterProperty": "Exclusion.ExclusionName",
                      "property": "ExclusionId",
                      "sortProperty": "Exclusion.ExclusionName",
                      "template": "${data.Exclusion?.ExclusionName}",
                      "title": "Exclusion",
                      "type": "string"
                    }
                  ],
                  "confirmDelete": true,
                  "confirmDeleteCancelText": "Cancel",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Are you sure you want to delete this record?",
                  "confirmDeleteTitle": "Confirmation",
                  "containsText": "Contains",
                  "count": "${getStateExclusionsCount}",
                  "data": "${getStateExclusionsResult}",
                  "emptyText": "No records to display.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "add": [
                      {
                        "path": "add-state-exclusion",
                        "type": "popup"
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete StateExclusion",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "StateExclusionsDatabase.deleteStateExclusion",
                        "parameters": [
                          {
                            "name": "Id",
                            "value": "${event.Id}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "StateExclusion deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "error": [
                          {
                            "detail": "Unable to load StateExclusions",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "StateExclusionsDatabase.getStateExclusions",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "State,Exclusion"
                          }
                        ],
                        "then": [
                          {
                            "name": "getStateExclusionsResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getStateExclusionsCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "Id",
                            "value": "${event.Id}"
                          }
                        ],
                        "path": "edit-state-exclusion",
                        "type": "popup"
                      }
                    ]
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid0",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.grid0.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "Main",
  "name": "StateExclusion"
}